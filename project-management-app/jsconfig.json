{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "@/*": ["src/*"],
            "@components/*": ["src/components/*"],
            "@pages/*": ["src/pages/*"],
            "@context/*": ["src/context/*"],
            "@assets/*": ["src/assets/*"],
            "@data/*": ["src/data/*"],
            "@utils/*": ["src/utils/*"],
            "@CSS/*": ["src/CSS/*"],
            "@navigation/*": ["navigation/*"]
        }
    }
}
